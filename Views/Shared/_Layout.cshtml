<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Quiz</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet">

    <!-- Lib CSS -->
    <link href="~/lib/fontawesome-pro-513/css/all.min.css" rel="stylesheet" />
    <link href="~/lib/owlcarousel/dist/assets/owl.carousel.min.css" rel="stylesheet" asp-append-version="true" />
    <link href="~/lib/owlcarousel/dist/assets/owl.theme.default.min.css" rel="stylesheet" asp-append-version="true" />
    <link href="~/lib/aos/dist/aos.css" rel="stylesheet" asp-append-version="true" />
    <link href="~/css/custom.css" rel="stylesheet" />
    <link href="~/css/main.css" rel="stylesheet" asp-append-version="true" />
    <link href="~/lib/jqueryui/jquery-ui.min.css" rel="stylesheet" />
    @RenderSection("Styles", required: false)
</head>
<body>
    <partial name="_Header" />
    @RenderBody()
    <partial name="_Footer" />


    <partial name="_Header" />
    <partial name="_Footer" />
    
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/lib/jquery/dist/jquery.min.js" asp-append-version="true"></script>
    <script src="~/lib/owlcarousel/dist/owl.carousel.min.js" asp-append-version="true"></script>
    <script src="~/lib/sweetalert/sweetalert.min.js" asp-append-version="true"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js" asp-append-version="true"></script>
    <script src="~/lib/aos/dist/aos.js" asp-append-version="true"></script>
    <script src="~/lib/bootstrap-select/dist/js/bootstrap-select.min.js" asp-append-version="true"></script>
    <script src="~/lib/jqueryui/jquery-ui.min.js"></script>
    <script src="~/lib/nouislider/dist/jquery.ui.touch-punch.min.js"></script>
    <script src="~/js/main.min.js" asp-append-version="true"></script>
    <script src="~/lib/igjs/src/custom.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
